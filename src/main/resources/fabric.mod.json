{
	"schemaVersion": 1,
	"id": "rhseungslib",
	"version": "${version}",
	"name": "RhseungsLib",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/rhseungslib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.rhseung.rhseungslib.MainInitializer",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "net.rhseung.rhseungslib.ClientInitializer",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.rhseung.rhseungslib.DataGenerator",
				"adapter": "kotlin"
			}
		],
		"preLaunch": [
			"com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
		]
	},
	"mixins": [
		"rhseungslib.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.14",
		"minecraft": "~1.19.3",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.8.10"
	},
	"suggests": {
		"another-mod": "*"
	}
}